TABFILE = en-hy-dict.tab

all:
	cat A > $(TABFILE)
	cat B >> $(TABFILE)
	cat C >> $(TABFILE)
	cat D >> $(TABFILE)
	cat E >> $(TABFILE)
	cat F >> $(TABFILE)
	cat G >> $(TABFILE)
	cat H >> $(TABFILE)
	cat I >> $(TABFILE)
	cat J >> $(TABFILE)
	cat K >> $(TABFILE)
	cat L >> $(TABFILE)
	cat M >> $(TABFILE)
	cat N >> $(TABFILE)
	cat O >> $(TABFILE)
	cat P >> $(TABFILE)
	cat Q >> $(TABFILE)
	cat R >> $(TABFILE)
	cat S >> $(TABFILE)
	cat T >> $(TABFILE)
	cat U >> $(TABFILE)
	cat V >> $(TABFILE)
	cat W >> $(TABFILE)
	cat X >> $(TABFILE)
	cat Y >> $(TABFILE)
	cat Z >> $(TABFILE)
	echo '' >> $(TABFILE)
	stardict_tabfile $(TABFILE)
